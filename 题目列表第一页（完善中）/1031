#include <iostream>

using namespace std;

int main()
{
    int num, digit1, digit2, digit3, reverse_num;
    cin >> num;

    digit1 = num / 100;      // 取百位数字
    digit2 = num / 10 % 10;  // 取十位数字
    digit3 = num % 10;       // 取个位数字

    reverse_num = digit3 * 100 + digit2 * 10 + digit1;  // 计算反向数字

    cout << reverse_num << endl;
    return 0;
}

